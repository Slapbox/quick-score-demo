{
  "env": {
    "test": {
      "presets": [
        "@babel/preset-env"
      ]
    }
  },
  "presets": [
    "@babel/preset-react",
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": 62
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-object-rest-spread",
    "styled-components"
  ]
}
